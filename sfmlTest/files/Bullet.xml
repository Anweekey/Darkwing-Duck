<!DOCTYPE SpriteDecomposer>
<sprites image="Bullet.png" transparentColor="FFFFFF">
 <animation title="Bullet" delay="150">
  <cut w="7" x="-0.5" y="6" h="5" row="1" col="1"/>
  <cut w="6" x="11" y="6" h="5" row="1" col="1"/>
  <cut w="6" x="22" y="5" h="8" row="1" col="1"/>
